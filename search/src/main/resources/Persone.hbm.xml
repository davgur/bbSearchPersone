<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="search.Persone" table="persone">
		<id column="id" name="id" type="java.lang.Long">
			<generator class="increment" />
		</id>
		<property column="name" name="name" type="java.lang.String" />
		<property column="translit" name="translit" type="java.lang.String" />
		<property column="telephone" name="telephone" type="java.lang.String" />
		<property column="carNumber" name="carNumber" type="java.lang.String" />
		<property column="status" name="status" type="java.lang.String" />
	</class>
</hibernate-mapping>